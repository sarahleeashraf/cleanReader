{
  "name": "qr-ome demo",
  "version": "0.4",
  "description": "Shows you QR codes for links",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery-1.3.2.min.js", "qrcode.js"],
      "css": ["qrcode.css"]
    }
  ],
  "background_page": "background.html",
  "browser_action": {
    "default_icon": "off.png",
    "default_title": "Click to enable QR-ome!"
  },
  "permissions": [
    "tabs"
  ]
}